{
  "name": "jira",
  "version": "2.0.0-beta.1",
  "description": "Daily ticket state dashboard for JIRA",
  "author": "ULIVZ <chl814@foxmail.com>",
  "private": true,
  "scripts": {
    "feonly:dev": "APP_MODE=feOnly webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "feonly:build": "APP_MODE=feOnly node build/build.js",
    "dev:fe": "PORT=8081 webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev:api": "NODE_ENV=dev API_PORT=3000 node build/dev/dev:api.js",
    "dev": "NODE_ENV=dev API_PORT=3000 PORT=8081 webpack-dev-server --inline --progress --config build/dev/dev.js",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "build": "node build/build.js",
    "release": "npm install && node build/build.js && pm2 start api/bin/www -i max",
    "ci": "npm install && node build/build.js && pm2 reload www"
  },
